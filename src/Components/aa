{this.props!==undefined&&this.props.itemIds!==undefined?<ScrollMenu
  arrowLeft={<div style={{ fontSize: "30px" }}>{" < "}</div>}
  arrowRight={<div style={{ fontSize: "30px" }}>{" > "}</div>}
